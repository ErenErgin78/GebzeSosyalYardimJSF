--------------------------------------------------------
--  DDL for Procedure INSERT_KISI_TEMEL
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "GEBZEADMIN"."INSERT_KISI_TEMEL" (
    p_KIMLIK_NO IN KISI_TEMEL.KIMLIK_NO%TYPE,
    p_ISIM IN KISI_TEMEL.ISIM%TYPE,
    p_SOYISIM IN KISI_TEMEL.SOYISIM%TYPE,
    p_CINSIYET IN KISI_TEMEL.CINSIYET%TYPE,
    p_MEDENI_DURUM_ID IN KISI_TEMEL.MEDENI_DURUM_ID%TYPE,
    p_CILT_NO IN KISI_TEMEL.CILT_NO%TYPE,
    p_AILE_SIRA_NO IN KISI_TEMEL.AILE_SIRA_NO%TYPE,
    p_SIRA_NO IN KISI_TEMEL.SIRA_NO%TYPE,
    p_DOGUM_TARIHI IN KISI_TEMEL.DOGUM_TARIHI%TYPE,
    p_KISI_ILETISIM_ID IN KISI_TEMEL.KISI_ILETISIM_ID%TYPE,
    p_KISI_ADRES_ID IN KISI_TEMEL.KISI_ADRES_ID%TYPE,
    p_KISI_YAKINLAR_ID IN KISI_TEMEL.KISI_YAKINLAR_ID%TYPE
) AS
BEGIN
    INSERT INTO KISI_TEMEL (
        KIMLIK_NO, ISIM, SOYISIM, CINSIYET, MEDENI_DURUM_ID, CILT_NO, AILE_SIRA_NO, SIRA_NO, DOGUM_TARIHI, KISI_ILETISIM_ID, KISI_ADRES_ID, KISI_YAKINLAR_ID
    ) VALUES (
        p_KIMLIK_NO, p_ISIM, p_SOYISIM, p_CINSIYET, p_MEDENI_DURUM_ID, p_CILT_NO, p_AILE_SIRA_NO, p_SIRA_NO, p_DOGUM_TARIHI, p_KISI_ILETISIM_ID, p_KISI_ADRES_ID, p_KISI_YAKINLAR_ID
    );
END;

