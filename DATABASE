--  DDL for Procedure INSERT_MURACAAT
--------------------------------------------------------
create or replace PROCEDURE              "INSERT_MURACAAT" (
    p_kisi_temel_id IN NUMBER,
    p_muracaat_bilgi_id IN NUMBER
) AS
BEGIN
    INSERT INTO MURACAAT (KISI_TEMEL_ID, MURACAAT_BILGI_ID)
    VALUES (p_kisi_temel_id, p_muracaat_bilgi_id);
    END;
--------------------------------------------------------
--  DDL for Procedure INSERT_MURACAAT_BILGI
--------------------------------------------------------
create or replace PROCEDURE              "INSERT_MURACAAT_BILGI" (
    p_arsiv_dosya_no IN NUMBER,
    p_muracaat_tip_id IN NUMBER,
    p_aciklama IN VARCHAR2,
    p_muracaat_tarihi IN DATE,
    p_muracaat_bilgi_id OUT NUMBER
) AS
BEGIN
    INSERT INTO MURACAAT_BILGI (ARSIV_DOSYA_NO, MURACAAT_TIP_ID, ACIKLAMA, MURACAAT_TARIHI)
    VALUES (p_arsiv_dosya_no, p_muracaat_tip_id, p_aciklama, p_muracaat_tarihi)
    RETURNING MURACAAT_BILGI_ID INTO p_muracaat_bilgi_id;
    END;
