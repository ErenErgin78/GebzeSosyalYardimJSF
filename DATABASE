--  DDL for Procedure GET_KISI_INFO
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "GEBZEADMIN"."GET_KISI_INFO" (
    p_kimlik_no IN NUMBER,
    p_dogum_tarihi OUT DATE,
    p_isim OUT VARCHAR2,
    p_soyisim OUT VARCHAR2,
    p_cinsiyet OUT CHAR,
    p_aile_sira_no OUT NUMBER,
    p_cilt_no OUT VARCHAR2,
    p_medeni_durum_id OUT NUMBER,
    p_sira_no OUT NUMBER,
    p_anne_isim OUT VARCHAR2,
    p_baba_isim OUT VARCHAR2,
    p_es_isim OUT VARCHAR2,
    p_es_soyisim OUT VARCHAR2,
    p_ilce OUT VARCHAR2,
    p_tarif OUT VARCHAR2,
    p_site OUT VARCHAR2,
    p_kapi_no OUT NUMBER,
    p_daire_no OUT NUMBER,
    p_adres_no OUT NUMBER,
    p_mahalle_id OUT NUMBER,
    p_ev_telefon OUT NUMBER,
    p_cep_telefon OUT NUMBER,
    p_eposta OUT VARCHAR2
) AS
BEGIN
    SELECT KT.DOGUM_TARIHI, KT.ISIM, KT.SOYISIM, KT.CINSIYET, KT.AILE_SIRA_NO,
           KT.CILT_NO, KT.MEDENI_DURUM_ID, KT.SIRA_NO, KY.ANNE_ISIM, KY.BABA_ISIM,
           KY.ES_ISIM, KY.ES_SOYISIM, KA.ILCE, KA.TARIF, KA.SITE, KA.KAPI_NO,
           KA.DAIRE_NO, KA.ADRES_NO, KA.KISI_ADRES_MAHALLE_ID, KI.EV_TELEFON,
           KI.CEP_TELEFON, KI.EPOSTA
      INTO p_dogum_tarihi, p_isim, p_soyisim, p_cinsiyet, p_aile_sira_no, p_cilt_no,
           p_medeni_durum_id, p_sira_no, p_anne_isim, p_baba_isim, p_es_isim, p_es_soyisim,
           p_ilce, p_tarif, p_site, p_kapi_no, p_daire_no, p_adres_no, p_mahalle_id,
           p_ev_telefon, p_cep_telefon, p_eposta
      FROM KISI_TEMEL KT
      JOIN KISI_ILETISIM KI ON KT.KISI_ILETISIM_ID = KI.KISI_ILETISIM_ID
      JOIN KISI_YAKINLAR KY ON KT.KISI_YAKINLAR_ID = KY.KISI_YAKINLAR_ID
      JOIN KISI_ADRES KA ON KT.KISI_ADRES_ID = KA.KISI_ADRES_ID
     WHERE KT.KIMLIK_NO = p_kimlik_no;
END;

/
--------------------------------------------------------
--  DDL for Procedure INSERT_ADRES_MAHALLE_SOKAK
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "GEBZEADMIN"."INSERT_ADRES_MAHALLE_SOKAK" (
    p_mahalle_id IN KISI_MAHALLE_SOKAK.MAHALLE_ID%TYPE,
    p_sokak_isim IN KISI_MAHALLE_SOKAK.SOKAK_ISIM%TYPE,
    p_aktif IN KISI_MAHALLE_SOKAK.AKTIF%TYPE
) AS
BEGIN
    INSERT INTO KISI_MAHALLE_SOKAK (MAHALLE_ID, SOKAK_ISIM, AKTIF)
    VALUES (p_mahalle_id, p_sokak_isim, p_aktif);
END;

/
--------------------------------------------------------
--  DDL for Procedure INSERT_KISI_ADRES
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "GEBZEADMIN"."INSERT_KISI_ADRES" (
    p_ilce IN KISI_ADRES.ILCE%TYPE,
    p_mahalle_id IN KISI_ADRES.KISI_ADRES_MAHALLE_ID%TYPE,
    p_tarif IN KISI_ADRES.TARIF%TYPE,
    p_site IN KISI_ADRES.SITE%TYPE,
    p_kapi_no IN KISI_ADRES.KAPI_NO%TYPE,
    p_daire_no IN KISI_ADRES.DAIRE_NO%TYPE,
    p_adres_no IN KISI_ADRES.ADRES_NO%TYPE,
    p_mahalle_sokak_id IN KISI_ADRES.KISI_MAHALLE_SOKAK_ID%TYPE,
    p_kisi_adres_id OUT KISI_ADRES.KISI_ADRES_ID%TYPE
) AS
BEGIN
    INSERT INTO KISI_ADRES (
        ILCE, 
        KISI_ADRES_MAHALLE_ID, 
        TARIF, 
        SITE, 
        KAPI_NO, 
        DAIRE_NO, 
        ADRES_NO, 
        KISI_MAHALLE_SOKAK_ID
    ) 
    VALUES (
        p_ilce, 
        p_mahalle_id, 
        p_tarif, 
        p_site, 
        p_kapi_no, 
        p_daire_no, 
        p_adres_no, 
        p_mahalle_sokak_id
    )
    RETURNING KISI_ADRES_ID INTO p_kisi_adres_id;
END;

/
--------------------------------------------------------
--  DDL for Procedure INSERT_KISI_ADRES_MAHALLE
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "GEBZEADMIN"."INSERT_KISI_ADRES_MAHALLE" (
   p_mahalle IN KISI_ADRES_MAHALLE.MAHALLE%TYPE,
   p_aktif IN KISI_ADRES_MAHALLE.AKTIF%TYPE
)AS
BEGIN
    INSERT INTO KISI_ADRES_MAHALLE (MAHALLE, AKTIF) VALUES (p_mahalle, p_aktif);
END;

/
--------------------------------------------------------
--  DDL for Procedure INSERT_KISI_ILETISIM
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "GEBZEADMIN"."INSERT_KISI_ILETISIM" (
    p_ev_telefon IN KISI_ILETISIM.EV_TELEFON%TYPE,
    p_cep_telefon IN KISI_ILETISIM.CEP_TELEFON%TYPE,
    p_eposta IN KISI_ILETISIM.EPOSTA%TYPE,
    p_kisi_iletisim_id OUT KISI_ILETISIM.KISI_ILETISIM_ID%TYPE
) AS
BEGIN
    INSERT INTO KISI_ILETISIM (EV_TELEFON, CEP_TELEFON, EPOSTA)
    VALUES (p_ev_telefon, p_cep_telefon, p_eposta)
    RETURNING KISI_ILETISIM_ID INTO p_kisi_iletisim_id;
END;

