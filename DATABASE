        BABA_ISIM, ANNE_ISIM, ES_ISIM, ES_SOYISIM
    ) VALUES (
        p_baba_isim, p_anne_isim, p_es_isim, p_es_soyisim
    )
    RETURNING KISI_YAKINLAR_ID INTO p_kisi_yakinlar_id;
END;

