--------------------------------------------------------
--  DDL for Procedure GET_KISI_INFO
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "GEBZEADMIN"."GET_KISI_INFO" (
    p_kimlik_no IN NUMBER,
    p_dogum_tarihi OUT DATE,
    p_isim OUT VARCHAR2,
    p_soyisim OUT VARCHAR2,
    p_cinsiyet OUT CHAR,
    p_aile_sira_no OUT NUMBER,
    p_cilt_no OUT VARCHAR2,
    p_medeni_durum_id OUT NUMBER,
    p_sira_no OUT NUMBER,
    p_anne_isim OUT VARCHAR2,
    p_baba_isim OUT VARCHAR2,
    p_es_isim OUT VARCHAR2,
    p_es_soyisim OUT VARCHAR2,
    p_ilce OUT VARCHAR2,
    p_tarif OUT VARCHAR2,
    p_site OUT VARCHAR2,
    p_kapi_no OUT NUMBER,
    p_daire_no OUT NUMBER,
    p_adres_no OUT NUMBER,
    p_mahalle_id OUT NUMBER,
    p_ev_telefon OUT NUMBER,
    p_cep_telefon OUT NUMBER,
    p_eposta OUT VARCHAR2
) AS
BEGIN
    SELECT KT.DOGUM_TARIHI, KT.ISIM, KT.SOYISIM, KT.CINSIYET, KT.AILE_SIRA_NO,
           KT.CILT_NO, KT.MEDENI_DURUM_ID, KT.SIRA_NO, KY.ANNE_ISIM, KY.BABA_ISIM,
           KY.ES_ISIM, KY.ES_SOYISIM, KA.ILCE, KA.TARIF, KA.SITE, KA.KAPI_NO,
           KA.DAIRE_NO, KA.ADRES_NO, KA.KISI_ADRES_MAHALLE_ID, KI.EV_TELEFON,
           KI.CEP_TELEFON, KI.EPOSTA
      INTO p_dogum_tarihi, p_isim, p_soyisim, p_cinsiyet, p_aile_sira_no, p_cilt_no,
           p_medeni_durum_id, p_sira_no, p_anne_isim, p_baba_isim, p_es_isim, p_es_soyisim,
           p_ilce, p_tarif, p_site, p_kapi_no, p_daire_no, p_adres_no, p_mahalle_id,
           p_ev_telefon, p_cep_telefon, p_eposta
      FROM KISI_TEMEL KT
      JOIN KISI_ILETISIM KI ON KT.KISI_ILETISIM_ID = KI.KISI_ILETISIM_ID
      JOIN KISI_YAKINLAR KY ON KT.KISI_YAKINLAR_ID = KY.KISI_YAKINLAR_ID
      JOIN KISI_ADRES KA ON KT.KISI_ADRES_ID = KA.KISI_ADRES_ID
     WHERE KT.KIMLIK_NO = p_kimlik_no;
END;

