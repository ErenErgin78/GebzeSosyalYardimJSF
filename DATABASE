--  DDL for Procedure INSERT_MURACAAT_BILGI
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "GEBZEADMIN"."INSERT_MURACAAT_BILGI" (
    p_arsiv_dosya_no IN NUMBER,
    p_muracaat_tip_id IN NUMBER,
    p_bolge IN NUMBER,
    p_aciklama IN VARCHAR2,
    p_muracaat_tarihi IN DATE,
    p_muracaat_bilgi_id OUT NUMBER
) AS
BEGIN
    INSERT INTO MURACAAT_BILGI (ARSIV_DOSYA_NO, MURACAAT_TIP_ID, BOLGE, ACIKLAMA, MURACAAT_TARIHI)
    VALUES (p_arsiv_dosya_no, p_muracaat_tip_id, p_bolge, p_aciklama, p_muracaat_tarihi)
    RETURNING MURACAAT_BILGI_ID INTO p_muracaat_bilgi_id;
END;

