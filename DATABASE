    p_sokak_isim IN KISI_MAHALLE_SOKAK.SOKAK_ISIM%TYPE,
    p_aktif IN KISI_MAHALLE_SOKAK.AKTIF%TYPE
) AS
BEGIN
    INSERT INTO KISI_MAHALLE_SOKAK (MAHALLE_ID, SOKAK_ISIM, AKTIF)
    VALUES (p_mahalle_id, p_sokak_isim, p_aktif);
END;

