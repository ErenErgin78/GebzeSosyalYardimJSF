--------------------------------------------------------
--  DDL for Procedure INSERT_KISI_ADRES
--------------------------------------------------------
create or replace PROCEDURE INSERT_KISI_ADRES (
    p_ilce IN KISI_ADRES.ILCE%TYPE,
    p_mahalle_id IN KISI_ADRES.KISI_ADRES_MAHALLE_ID%TYPE,
    p_tarif IN KISI_ADRES.TARIF%TYPE,
    p_site IN KISI_ADRES.SITE%TYPE,
    p_kapi_no IN KISI_ADRES.KAPI_NO%TYPE,
    p_daire_no IN KISI_ADRES.DAIRE_NO%TYPE,
    p_adres_no IN KISI_ADRES.ADRES_NO%TYPE,
    p_mahalle_sokak_id IN KISI_ADRES.KISI_MAHALLE_SOKAK_ID%TYPE,
    p_kisi_adres_id OUT KISI_ADRES.KISI_ADRES_ID%TYPE
) AS
BEGIN
    INSERT INTO KISI_ADRES (
        ILCE, 
        KISI_ADRES_MAHALLE_ID, 
        TARIF, 
        SITE, 
        KAPI_NO, 
        DAIRE_NO, 
        ADRES_NO, 
        KISI_MAHALLE_SOKAK_ID
    ) 
    VALUES (
        p_ilce, 
        p_mahalle_id, 
        p_tarif, 
        p_site, 
        p_kapi_no, 
        p_daire_no, 
        p_adres_no, 
        p_mahalle_sokak_id
    )
    RETURNING KISI_ADRES_ID INTO p_kisi_adres_id;
END;
--------------------------------------------------------
--  DDL for Procedure INSERT_KISI_ILETISIM
--------------------------------------------------------

create or replace PROCEDURE INSERT_KISI_ILETISIM (
    p_ev_telefon IN KISI_ILETISIM.EV_TELEFON%TYPE,
    p_cep_telefon IN KISI_ILETISIM.CEP_TELEFON%TYPE,
    p_eposta IN KISI_ILETISIM.EPOSTA%TYPE,
    p_kisi_iletisim_id OUT KISI_ILETISIM.KISI_ILETISIM_ID%TYPE
) AS
BEGIN
    INSERT INTO KISI_ILETISIM (EV_TELEFON, CEP_TELEFON, EPOSTA)
    VALUES (p_ev_telefon, p_cep_telefon, p_eposta)
    RETURNING KISI_ILETISIM_ID INTO p_kisi_iletisim_id;
END;

