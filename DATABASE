--------------------------------------------------------
--  DDL for Procedure INSERT_KISI_ADRES
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "GEBZEADMIN"."INSERT_KISI_ADRES" (
    p_ilce IN KISI_ADRES.ILCE%TYPE,
    p_mahalle_id IN KISI_ADRES.KISI_ADRES_MAHALLE_ID%TYPE,
    p_cadde_sokak IN KISI_ADRES.CADDE_SOKAK%TYPE,
    p_tarif IN KISI_ADRES.TARIF%TYPE,
    p_site IN KISI_ADRES.SITE%TYPE,
    p_kapi_no IN KISI_ADRES.KAPI_NO%TYPE,
    p_daire_no IN KISI_ADRES.DAIRE_NO%TYPE,
    p_adres_no IN KISI_ADRES.ADRES_NO%TYPE,
    p_kisi_adres_id OUT KISI_ADRES.KISI_ADRES_ID%TYPE
) AS
BEGIN
    INSERT INTO KISI_ADRES (ILCE, KISI_ADRES_MAHALLE_ID, CADDE_SOKAK, TARIF, SITE, KAPI_NO, DAIRE_NO, ADRES_NO)
    VALUES (p_ilce, p_mahalle_id, p_cadde_sokak, p_tarif, p_site, p_kapi_no, p_daire_no, p_adres_no)
    RETURNING KISI_ADRES_ID INTO p_kisi_adres_id;
END;

