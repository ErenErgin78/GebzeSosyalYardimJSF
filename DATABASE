--  DDL for Procedure INSERT_KISI_YAKINLAR
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "GEBZEADMIN"."INSERT_KISI_YAKINLAR" (
    p_baba_isim IN KISI_YAKINLAR.BABA_ISIM%TYPE,
    p_anne_isim IN KISI_YAKINLAR.ANNE_ISIM%TYPE,
    p_es_isim IN KISI_YAKINLAR.ES_ISIM%TYPE,
    p_es_soyisim IN KISI_YAKINLAR.ES_SOYISIM%TYPE,
    p_kisi_yakinlar_id OUT KISI_YAKINLAR.KISI_YAKINLAR_ID%TYPE
) AS
BEGIN
